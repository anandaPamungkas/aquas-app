<%- include('partials/header') %>
    <div class="container body">

        <div class="main_container">
            <div class="col-md-3 left_col">
                <%- include('partials/sidebar-menu') %>
            </div>

            <%- include('partials/top-navigation') %>
                <div class="right_col" role="main">
                    <div class="page-title">
                        <div class="title_left">
                            <h3>Kelola Admin</h3>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <!--Start administrators list table-->
                    <div class="row administrator_list_container">

                        <div class="col-md-12 col-sm-12 ">
                            <div class="dashboard_graph">

                                <div class="row x_title">
                                    <div class="col-md-6">
                                        <h3>Daftar Administrator</h3>
                                    </div>
                                    <div class="col-md-6">
                                        <div id="add_admin_button" class="pull-right btn btn-primary">
                                            <a href="/add-admin">
                                                <i class="fa fa-user-plus"></i>
                                                <span>Tambah Admin</span> <b class="caret"></b>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12 col-sm-12 ">
                                    <% if (items.length > 0) {%>
                                        <table id="admins_data_table" class="table table-striped table-bordered" style="width:100%">
                                            <thead>
                                                <tr>
                                                    <th>Nama</th>
                                                    <th>E-mail</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% items.forEach((item, index) => { %>
                                                    <tr data-id="<%= item.id %>">
                                                        <td>
                                                            <%= item.nama %>
                                                        </td>
                                                        <td>
                                                            <%= item.email %>
                                                        </td>
                                                    </tr>
                                                    <% }) %>
                                            </tbody>
                                        </table>
                                        <% } else { %>
                                            <p class="text-center">No data found</p>
                                            <% } %>
                                </div>
                                <div class="clearfix"></div>
                            </div>
                        </div>

                        <br />
                    </div>
                    <!--End administrators list table-->
                    <br />
                </div>
                <%- include('partials/footer') %>